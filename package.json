{
  "name": "website-toolbox",
  "version": "0.4.3",
  "description": "A dead-simple framework for dealing with all sorts of \"website things\".",
  "main": "build/website-toolbox.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "test:badges": "npm run test && jest-coverage-badges",
    "build": "npm run test:badges && rollup --config"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json-summary", 
      "text",
      "lcov"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dougalcantara/website-toolbox.git"
  },
  "author": "Doug Alcantara",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dougalcantara/website-toolbox/issues"
  },
  "homepage": "https://github.com/dougalcantara/website-toolbox#readme",
  "browserslist": "> 0.25%, not dead",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "rollup": "^2.33.1",
    "rollup-plugin-uglify": "^6.0.4"
  }
}
